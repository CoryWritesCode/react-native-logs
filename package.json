    "logger",
    "react-native",
    "custom",
    "levels",
    "console",
    "debug"
  ],
  "author": "Onubo s.r.l. <www.onubo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onubo/react-native-logs/issues"
  },
  "homepage": "https://github.com/onubo/react-native-logs#readme",
  "devDependencies": {
    "coveralls": "^3.0.4",
    "jest": "^24.8.0",
    "typescript": "^3.5.2"
  }
}
